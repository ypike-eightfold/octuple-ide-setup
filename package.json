{
  "name": "octuple-ai-starter",
  "version": "2.2.0",
  "description": "Starter template for using Octuple design system with AI coding agents",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "extract-apis": "node scripts/extract-octuple-apis.cjs",
    "verify-components": "node scripts/extract-octuple-apis.cjs",
    "check-props": "bash scripts/check-component-props.sh",
    "check-version": "node scripts/check-octuple-version.cjs",
    "detect-changes": "node scripts/detect-api-changes.cjs",
    "save-snapshot": "node scripts/detect-api-changes.cjs --save",
    "playground": "echo 'Change App.tsx to import ComponentPlayground and run npm run dev'"
  },
  "dependencies": {
    "@eightfold.ai/octuple": "^2.54.2",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.21.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.10"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
